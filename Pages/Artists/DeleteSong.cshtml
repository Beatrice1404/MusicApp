@page "{id}"
@model MusicApp.Pages.Artists.DeleteSongModel
@{
    ViewData["Title"] = "Delete Song";
}

<h1>Delete Song</h1>

<p>Are you sure you want to delete the song "@Model.Song.title"?</p>

<form method="post">
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-page="./Albums" asp-route-id="@Model.Song.Id" class="btn btn-secondary">Cancel</a>
</form>
