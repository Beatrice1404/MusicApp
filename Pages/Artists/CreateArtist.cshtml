@page
@model MusicApp.Pages.Artists.CreateArtistModel
@{
    ViewData["Title"] = "Create Artist";
}

<h1>Create Artist</h1>

<form method="post">
    <div class="form-group">
        <label for="name">Name</label>
        <input type="text" class="form-control" id="name" name="Artist.name" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
